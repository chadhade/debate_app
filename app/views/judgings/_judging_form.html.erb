<!-- must pass in judging to partial!!! -->
<div class='judging_form'>
	<%= form_for judging, :remote => true, :url => submission_judging_path(judging) do |f| %>
		<div class = "field">
			Select a winner:<br>
			<%= f.select(:winner_id, [[judging.debate.creator.email, judging.debate.creator.id], [judging.debate.joiner.email, judging.debate.joiner.id]]) %><br /> <br>
			<%= f.label :comments %> <br/>
			<%= f.text_area :comments, :size => "10x2" %>
		</div>
		<div class = "action">
			<%= f.submit "Submit" %>
		</div>	
	<% end %>
</div>