<!-- must pass in judging to partial!!! -->
<div class='judging_form'>
	<%= form_for judging, :remote => true, :url => submission_judging_path(judging) do |f| %>
		<div class = "field">
			<%= f.label :winner_id %> <br />
			<%= f.number_field :winner_id %><br />
			<%= f.label :comments %> <br/>
			<%= f.text_field :comments %>
		</div>
		<div class = "action">
			<%= f.submit "submit" %>
		</div>	
	<% end %>
</div>