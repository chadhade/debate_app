<h3> Open Suggested Debates </h3>
<div class="containercontent smallcontent">
	<% if !matching.nil? %>
		<% if !matching[:suggested_debates].empty? %>
			<% @matching[:suggested_debates].each do |suggested_debate| %>
				<div class="debate_link" data-id="<%= suggested_debate.id %>"> 
					<li> <span class = "debate-link-topic">
					<%= link_to suggested_debate.tp.topic, suggested_debate %> </span>
					<% if suggested_debate.tp.position == true %>
						<span class = "pro">&nbsp</span>
					<% else %> 
					<% if suggested_debate.tp.position == false %>
						<span class = "con">&nbsp</span>
					<% end %>
					<% end %> 
					- <%= link_to "\"#{suggested_debate.topic}\"", suggested_debate %> </li>
				</div>
			<% end %>
		<% else %>
			No Suggested Debates
		<% end %>
	<% else %>
		No Suggested Debates
	<% end %>
</div>
