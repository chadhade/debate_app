<script type="text/javascript" charset="utf-8"> 
  jug.subscribe("debate_" + <%= debate.id %>, function(argument){
    $(".debate[data-id="+ <%= debate.id %> +"]").append(argument);
  });
</script>

<% if debate.creator?(current_debater) %>
  <script type="text/javascript" charset="utf-8"> 
	jug.subscribe("debate_" + <%= debate.id %> + "_creator", function(argument){
	  $(".debate[data-id="+ <%= debate.id %> +"]").append(argument);
	});
  </script>
<% end %>