<div class = "list_names">
	<h2>Debunker</h2>
</div>
<div class = "list_ranks">
	<h2>dScore</h2>
</div>
<div class = "list_ranks">
	<h2>% Won </h2>
</div>
<div class = "list_points">
	<h2>Judge Points</h2>
</div>
<div class = "list_joined">
	<h2>Joined</h2>
</div>

<% debaterranks.each do |debater| %>
	<div class = "list_names">
		<span class = "list_name"><span class = "gravatar"><%= gravatar_for debater[:debater], :size => 15 %></span><%= link_to debater[:debater].name, debater[:debater] %></span><br />
	</div>
	<div class = "list_ranks" style="color: #006666;">
		<%= debater[:debater][:rating] %>
	</div>
	<div class = "list_ranks">
		<%= (((debater[:rank] * 100.0).round / 100.0) * 100) if !debater[:rank].nil? %>%<br />
	</div>
	<div class = "list_points" style="color: #006666;">
		<%= debater[:judge_points] %>
	</div>
	<div class = "list_joined">
		<%= debater[:joined].strftime("%b. %d, %Y") %> <br />
	</div>
<% end %> 
<div>&nbsp</div>

<div class="list_pagination">
	<%= will_paginate debaterranks %>
</div>