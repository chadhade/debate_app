<!DOCTYPE html>
<html>
<head>
  <title>#</title>
  <%= stylesheet_link_tag "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<div id="debater_nav">
    <nav class="round">
		<% if debater_signed_in? %>
	        <%= current_debater.email %> -->
			<%= link_to 'Profile', current_debater %> |
			<%= link_to 'Trackings', debater_trackings_path(current_debater) %> |
	        <%= link_to 'Sign out', destroy_debater_session_path, :method => :delete %> |
			<%= link_to 'Debaters', debaters_path %> |
	    <% else %>
	        <%= link_to 'Register', new_debater_registration_path %> | <%= link_to 'Sign in', new_debater_session_path %> |
	    	<%= link_to "Sign in with Facebook", debater_omniauth_authorize_path(:facebook) %>
			<%= link_to "Sign in with Twitter", debater_omniauth_authorize_path(:twitter) %>
		<% end %>
		<%= link_to 'List of debates', debates_path %>
		<%= link_to 'Judgings index', judgings_path %> |
		<%= link_to 'Landing page', 'pages/landing' %>
	</nav>
</div>
<br />
<%= yield %>

</body>
</html>
